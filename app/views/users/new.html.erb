<h1>Sign Up</h1>

<form action='<%= users_url %>' method="POST" accept-charset="utf-8">
	<input type="hidden"
	         name="authenticity_token"
	         value="<%= form_authenticity_token %>">
	 <label for='input_email'>Email</label>
	 <input type='text' id="input_email" name="user[email]" value= '<%= @user.email %>'>
	 
	 <label for='input_password'>Password</label>
	 <input type='password' id="input_password" name="user[password]" value= '<%= @user.password %>'>
	<p><input type="submit" value="Sign Up"></p>
</form>